snippet circuit "circuitikz template"
\\documentclass{standalone}
\\usepackage[nooldvoltagedirection]{circuitikz}
\\begin{document}
	\\begin{circuitikz} \\draw
		${1:(0,2) node[or port] (or1) {0}}
		${2:(or1.in 1) node[left=0.25em] (A) {A}}
		${3:(or1.in 2) node[left=0.25em] (B) {B}}
		;
	\\end{circuitikz}

\\end{document}
endsnippet

snippet frame "\basic frame setup"
\\frame{\frametitle{$1}
	$0
}
endsnippet

snippet note "\frame note setup"
\\note{%
	\\begin{itemize}
		\\item $0
	\\end{itemize}
}
endsnippet

snippet align "\begin{align}..."
\\begin{align${1:*}}
	$0
\\end{align$1}
endsnippet

snippet bib "\bibliographystyle{IEEETrans}..."
\\bibliographystyle{${1:IEEEtran}}
\\bibliography{$2}
$0
endsnippet

snippet standalone "\documentclass{standalone}..."
\\documentclass{standalone}
\\begin{document}
$0
\\end{document}
endsnippet

snippet gls "\gls{...}"
\\gls{$1} $0
endsnippet

snippet glsp "\glspl{...}"
\\glspl{$1} $0
endsnippet
