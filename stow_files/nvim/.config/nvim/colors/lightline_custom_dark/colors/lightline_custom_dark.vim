hi clear
if exists("syntax_on")
  syntax reset
endif
let g:colors_name="lightline_custom_dark"


let Italic = ""
if exists('g:lightline_custom_dark_italic')
  let Italic = "italic"
endif
let g:lightline_custom_dark_italic = get(g:, 'lightline_custom_dark_italic', 0)

let Bold = ""
if exists('g:lightline_custom_dark_bold')
  let Bold = "bold"
endif

let g:lightline_custom_dark_bold = get(g:, 'lightline_custom_dark_bold', 0)


